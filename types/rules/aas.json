[
  {
    "id": "aas_root",
    "operator": "all",
    "rules": [
      {
        "id": "aas_citizenship_vietnam",
        "type": "select",
        "field": "citizenship",
        "value": "citizenshipyes",
        "message": "Must be a Vietnamese citizen."
      },
      {
        "id": "aas_no_australia_status",
        "type": "select",
        "field": "australiaPrOrCitizen",
        "value": "australiaPrNo",
        "message": "Cannot hold or be applying for Australian citizenship or PR."
      },
      {
        "id": "aas_spouse_no_au_nz_status",
        "type": "select",
        "field": "spousepr",
        "value": "No",
        "message": "Spouse cannot hold Australian or New Zealand citizenship or PR."
      },
      {
        "id": "aas_not_military",
        "type": "select",
        "field": "military",
        "value": "militaryno",
        "message": "Active military or police personnel are not eligible."
      },
      {
        "id": "aas_no_criminal_record",
        "type": "select",
        "field": "banATungCoTienAnTienSuChua",
        "value": "criminalno",
        "message": "Must not have a criminal record or investigation."
      },
      {
        "id": "aas_no_previous_australia_scholarship",
        "operator": "any",
        "message": "Not eligible if you previously received an Australian Government long-term scholarship.",
        "severity": "blocker",
        "rules": [
          {
            "id": "aas_never_gov_scholarship",
            "type": "select",
            "field": "govscholarship",
            "value": "govescholarno",
            "message": "Confirm you have not received a government scholarship."
          },
          {
            "id": "aas_gov_scholarship_england",
            "operator": "all",
            "rules": [
              {
                "id": "aas_gov_yes_england_flag",
                "type": "select",
                "field": "govscholarship",
                "value": "govescholaryes",
                "message": "You indicated you received a government scholarship."
              },
              {
                "id": "aas_gov_country_england",
                "type": "select",
                "field": "countryscholarship",
                "value": "Scholarengland",
                "message": "Only non-Australian government scholarships are allowed."
              }
            ]
          },
          {
            "id": "aas_gov_scholarship_america",
            "operator": "all",
            "rules": [
              {
                "id": "aas_gov_yes_america_flag",
                "type": "select",
                "field": "govscholarship",
                "value": "govescholaryes",
                "message": "You indicated you received a government scholarship."
              },
              {
                "id": "aas_gov_country_america",
                "type": "select",
                "field": "countryscholarship",
                "value": "Scholaramerica",
                "message": "Only non-Australian government scholarships are allowed."
              }
            ]
          },
          {
            "id": "aas_gov_scholarship_france",
            "operator": "all",
            "rules": [
              {
                "id": "aas_gov_yes_france_flag",
                "type": "select",
                "field": "govscholarship",
                "value": "govescholaryes",
                "message": "You indicated you received a government scholarship."
              },
              {
                "id": "aas_gov_country_france",
                "type": "select",
                "field": "countryscholarship",
                "value": "Scholarfrance",
                "message": "Only non-Australian government scholarships are allowed."
              }
            ]
          },
          {
            "id": "aas_gov_scholarship_other",
            "operator": "all",
            "rules": [
              {
                "id": "aas_gov_yes_other_flag",
                "type": "select",
                "field": "govscholarship",
                "value": "govescholaryes",
                "message": "You indicated you received a government scholarship."
              },
              {
                "id": "aas_gov_country_other",
                "type": "select",
                "field": "countryscholarship",
                "value": "Scholarother",
                "message": "Only non-Australian government scholarships are allowed."
              }
            ]
          }
        ]
      },
      {
        "id": "aas_group_selector",
        "operator": "any",
        "message": "Did not meet GPA/experience/English requirements for any applicant group.",
        "severity": "blocker",
        "rules": [
          {
            "id": "aas_branch_disability",
            "operator": "any",
            "rules": [
              {
                "id": "aas_branch_disability_requirements",
                "operator": "all",
                "message": "Did not meet requirements for the disability group.",
                "severity": "blocker",
                "rules": [
                  {
                    "id": "aas_branch_disability_flag",
                    "type": "select",
                    "field": "nhomyeuthe",
                    "value": "khuyettat",
                    "message": "Applies to disability group."
                  },
                  {
                    "id": "aas_disability_degree",
                    "operator": "any",
                    "message": "Disability group accepts Bachelor or Master degree.",
                    "severity": "blocker",
                    "rules": [
                      {
                        "id": "aas_disability_degree_bachelor",
                        "type": "select",
                        "field": "highestQualification",
                        "value": "Bachelor",
                        "message": "Disability group accepts Bachelor or Master."
                      },
                      {
                        "id": "aas_disability_degree_master",
                        "type": "select",
                        "field": "highestQualification",
                        "value": "Master",
                        "message": "Disability group accepts Bachelor or Master."
                      }
                    ]
                  },
                  {
                    "id": "aas_disability_gpa",
                    "type": "minScore",
                    "field": "gpa",
                    "value": 6,
                    "message": "Disability group requires GPA = 6.0."
                  },
                  {
                    "id": "aas_disability_experience",
                    "type": "minScore",
                    "field": "workingtime",
                    "value": 12,
                    "message": "Disability group requires at least 12 months of experience."
                  }
                ]
              },
              {
                "id": "aas_branch_disability_not_remote",
                "type": "select",
                "field": "nhomyeuthe",
                "value": "vungkhokhan"
              },
              {
                "id": "aas_branch_disability_not_normal",
                "type": "select",
                "field": "nhomyeuthe",
                "value": "khongyeuthe"
              }
            ]
          },
          {
            "id": "aas_branch_remote",
            "operator": "any",
            "rules": [
              {
                "id": "aas_branch_remote_requirements",
                "operator": "all",
                "message": "Did not meet requirements for the remote area group.",
                "severity": "blocker",
                "rules": [
                  {
                    "id": "aas_branch_remote_flag",
                    "type": "select",
                    "field": "nhomyeuthe",
                    "value": "vungkhokhan",
                    "message": "Applies to remote area group."
                  },
                  {
                    "id": "aas_remote_degree",
                    "type": "select",
                    "field": "highestQualification",
                    "value": "Bachelor",
                    "message": "Remote area group requires Bachelor degree."
                  },
                  {
                    "id": "aas_remote_gpa",
                    "type": "minScore",
                    "field": "gpa",
                    "value": 6,
                    "message": "Remote area group requires GPA = 6.0."
                  },
                  {
                    "id": "aas_remote_experience",
                    "type": "minScore",
                    "field": "workingtime",
                    "value": 12,
                    "message": "Remote area group requires at least 12 months of experience."
                  }
                ]
              },
              {
                "id": "aas_branch_remote_not_disability",
                "type": "select",
                "field": "nhomyeuthe",
                "value": "khuyettat"
              },
              {
                "id": "aas_branch_remote_not_normal",
                "type": "select",
                "field": "nhomyeuthe",
                "value": "khongyeuthe"
              }
            ]
          },
          {
            "id": "aas_branch_normal",
            "operator": "any",
            "rules": [
              {
                "id": "aas_branch_normal_requirements",
                "operator": "all",
                "message": "Did not meet requirements for the general applicant group.",
                "severity": "blocker",
                "rules": [
                  {
                    "id": "aas_branch_normal_flag",
                    "type": "select",
                    "field": "nhomyeuthe",
                    "value": "khongyeuthe",
                    "message": "Applies to general applicants."
                  },
                  {
                    "id": "aas_normal_degree",
                    "type": "select",
                    "field": "highestQualification",
                    "value": "Bachelor",
                    "message": "General group requires highest qualification Bachelor."
                  },
                  {
                    "id": "aas_normal_employer_rules",
                    "operator": "any",
                    "message": "General group: GPA/experience/English not met for employer type (foreign company applicants only eligible under disability group).",
                    "severity": "blocker",
                    "rules": [
                      {
                        "id": "aas_normal_gov_levels",
                        "operator": "all",
                        "rules": [
                          {
                            "id": "aas_normal_gov_levels_match",
                            "type": "select",
                            "field": "employmenttype",
                            "value": "gov_levels",
                            "message": "Branch applies to government agencies."
                          },
                          {
                            "id": "aas_normal_gov_levels_gpa",
                            "type": "minScore",
                            "field": "gpa",
                            "value": 6.5,
                            "message": "Requires GPA = 6.5."
                          },
                          {
                            "id": "aas_normal_gov_levels_experience",
                            "type": "minScore",
                            "field": "workingtime",
                            "value": 24,
                            "message": "Requires at least 24 months of experience."
                          }
                        ]
                      },
                      {
                        "id": "aas_normal_vocational_school",
                        "operator": "all",
                        "rules": [
                          {
                            "id": "aas_normal_vocational_school_match",
                            "type": "select",
                            "field": "employmenttype",
                            "value": "vocational_school",
                            "message": "Branch applies to vocational schools."
                          },
                          {
                            "id": "aas_normal_vocational_school_gpa",
                            "type": "minScore",
                            "field": "gpa",
                            "value": 6.5,
                            "message": "Requires GPA = 6.5."
                          },
                          {
                            "id": "aas_normal_vocational_school_experience",
                            "type": "minScore",
                            "field": "workingtime",
                            "value": 24,
                            "message": "Requires at least 24 months of experience."
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
]
